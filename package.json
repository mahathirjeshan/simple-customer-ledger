{
  "name": "remix-electron-template",
  "description": "",
  "version": "0.0.0",
  "private": true,
  "main": "desktop/main.js",
  "scripts": {
    "clean": "del-cli dist desktop/build public/build",
    "predev": "prisma generate && prisma migrate deploy",
    "dev": "npm run clean && cross-env NODE_ENV=development npm-run-all --parallel --print-label --race dev:*",
    "dev:remix": "remix watch",
    "dev:nodemon": "wait-on file:desktop/main.js && nodemon .",
    "build": "npm run clean && remix build && electron-builder",
    "start": "cross-env NODE_ENV=production electron ."
  },
  "build": {
    "files": [
      "./build/*",
      "./desktop",
      "./public",
      "./remix.config.js",
      "./prisma/*"
    ],
    "extraResources": [
      "prisma/**/*",
      "node_modules/.prisma/**/*",
      "node_modules/@prisma/client/**/*"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.1",
    "@prisma/client": "5.3.1",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-scroll-area": "^1.0.4",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/node": "^1.16.1",
    "@remix-run/react": "^1.16.1",
    "@remix-run/server-runtime": "^1.16.1",
    "@remix-run/web-stream": ">=1.0.3",
    "@tanstack/react-table": "^8.10.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "date-fns": "^2.30.0",
    "decimal.js": "^10.4.3",
    "fast-glob": "^3.2.12",
    "isbot": "latest",
    "lucide-react": "^0.279.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.2",
    "remix-electron": "latest",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.16.1",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "electron": "^25.0.0",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.3.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.4",
    "wait-on": "^7.0.1"
  },
  "sideEffects": false
}
